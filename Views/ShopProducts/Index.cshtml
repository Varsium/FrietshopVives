@model IList<Product>

<a class="btn btn-primary" asp-action="Create">Add Product to shop</a>

<table class="table table-dark">
    <thead>
    <tr>
        <th>Price</th>
        <th>Ingredient</th>
        <th>Quantity</th>
        <th>Edit Product</th>
    </tr>
    </thead>
    <tbody>

    @foreach (var product in Model)
    {
        <partial name="ProductPartial" model="product" />
    
    }
    </tbody>
</table>